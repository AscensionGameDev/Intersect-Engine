    <file src="..\assets\Intersect.png" target="assets" />
    <file src="..\targets\FindIntersect.targets" target="build" />
    <file src="..\targets\AscensionGameDev.Intersect.Server.Framework.targets" target="build" />
    <file src="..\Intersect.Server.Framework\bin\Release\Intersect Core.dll" target="lib\net461" />
    <file src="..\Intersect.Server.Framework\bin\Release\Intersect.Server.Framework.dll" target="lib\net461" />
    <file src="..\Intersect.Server.Framework\bin\Release\Intersect Core.dll" target="ref\net461" />
    <file src="..\Intersect.Server.Framework\bin\Release\Intersect.Server.Framework.dll" target="ref\net461" />
  </files>
