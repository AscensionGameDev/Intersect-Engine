<?xml version="1.0"?>
<package >
  <metadata>
    <id>AscensionGameDev.Intersect.Server.Framework</id>
    <version>0.7.0.4-beta</version>
    <title>Intersect Server Framework</title>
    <description>Library for the public Intersect Server API</description>
    <authors>Ascension Game Dev</authors>
    <owners>Ascension Game Dev</owners>
    <copyright>Copyright Â©  2020 Ascension Game Dev</copyright>
    <tags>intersect agd server plugin framework</tags>
    <license type="file">LICENSE.md</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>

    <projectUrl>https://github.com/AscensionGameDev/Intersect-Engine</projectUrl>
    <repository type="git" url="https://github.com/AscensionGameDev/Intersect-Engine" />
    <icon>assets\Intersect.png</icon>

    <releaseNotes>Nuget release of Intersect.Server.Framework for server plugin development.</releaseNotes>

    <dependencies>
