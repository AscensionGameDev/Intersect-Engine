{
	"DetailedErrors": true,
	"Kestrel": {
		"AddServerHeader": true,
		"AllowResponseHeaderCompression": false,
		"DisableStringReuse": true,
		"Endpoints": {
			"Http": {
				"Url": "http://localhost:5400"
			},
			"Https": {
				"Certificate": {
					"Path": "self-signed.crt",
					"KeyPath": "self-signed.key"
				},
				"Url": "https://localhost:5433"
			}
		}
	},
	"Logging": {
		"Common": {
			"MinimumLevel": {
				"Default": "Debug",
				"Override": {
					"Intersect": "Verbose"
				}
			}
		}
	}
}
